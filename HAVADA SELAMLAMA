HAVADA SELAMLAMA
#include<Servo.h>
Servo servo1;
Servo servo2;
Servo servo3;
Servo servo4;
Servo servo5;
Servo servo6;
Servo servo7;
Servo servo8;
Servo servo9;
Servo servo10;
Servo servo11;
Servo servo12;
void setup() {
  servo1.attach(2);
  servo2.attach(3);
  servo5.attach(6);
  servo6.attach(7);
  servo7.attach(8);
  servo8.attach(9);
  servo11.attach(12);
  servo12.attach(13); 
}
void loop() {
  // ref:sol taraflar 0 ları 180 sağ taraflar 0
  servo1.write(70);   //sağ ön1
  servo7.write(70);  //sağ arka1
  servo6.write(110);   //sol ön1
  servo12.write(110);   //sol arka1
  delay(1000);
  servo2.write(70);   //sağ ön2
  servo8.write(60);  //sağ arka2
  servo5.write(180);   //sol ön2
  servo11.write(110);  //ssol arka2  
delay(700);
servo6.write(50);   //sol ön1
 delay(700);
